{
  "name": "zerosonesfun/flarum-essential-audio",
  "description": "Beautiful, minimal audio player BBCode.",
  "type": "flarum-extension",
  "keywords": ["discussion", "audio", "bbcode"],
  "license": "MIT",
  "authors": [
      {
          "name": "Billy Wilcosky",
          "homepage": "https://www.wilcosky.com",
          "role": "Creator"
      }
  ],
  "support": {
      "issues": "https://github.com/zerosonesfun/flarum-essential-audio/issues",
      "source": "https://github.com/zerosonesfun/flarum-essential-audio",
      "forum": "https://discuss.flarum.org"
  },
  "homepage": "https://www.wilcosky.com",
  "funding": [
      {
          "type": "website",
          "url": "https://www.wilcosky.com"
      }
  ],
  "require": {
      "flarum/core": "^0.1.0-beta.16 || ^1.0",
      "fof/upload": "*",
      "flarum/bbcode": "*"
  },
  "autoload": {
      "psr-4": {
          "ZerosOnesFun\\EssentialAudio\\": "src/"
      }
  },
  "extra": {
      "flarum-extension": {
          "title": "Essential Audio",
          "category": "feature",
          "icon": {
              "name": "fas fa-play-circle",
              "backgroundColor": "#ddd",
              "color": "#000"
          }
      }
  }
}
